{
  "name": "@kevinrpb/slide-show",
  "version": "0.0.1",
  "description": "WebComponent slide-show following open-wc recommendations",
  "author": "Kevin Romero Peces-Barba <hello@kevinrpb.me> (https://kevinrpb.me)",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "web-dev-server",
    "dev": "web-dev-server --hrm",
    "analyze": "wca analyze --outFile custom-elements.json --format json",
    "lint": "eslint --ext .js,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^0.3.12",
    "@web/dev-server": "^0.1.17",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "web-component-analyzer": "^1.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d5310f464ab4ae5290f853eb00acb8634402eba3"
}
